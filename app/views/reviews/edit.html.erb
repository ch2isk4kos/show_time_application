<div class="col-md-10 col-md-offset-1">
    <div class="edit-review-form">
        <h1>Edit Review</h1>
        <%= simple_form_for([@movie, @review]) do |f| %>
            <div class="rating-form">
                <%= f.input :rating %>
            </div>
            <%= f.input :comment %>
            <%= f.button :submit, class: 'btn btn-custom2 btn-info' %>
        <% end %>
    </div>
</div>
